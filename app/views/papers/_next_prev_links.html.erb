<nav class="round">
  <ul>
    <% if @feed.nil? || @feed.is_default? %>
      <li><%= link_to("<<< Previous day", papers_prev_path(date: @date)) %></li>
      <li><%= link_to("Next day >>>", papers_next_path(date: @date)) %></li>
    <% else %>
      <li><%= link_to("<<< Previous day", papers_prev_path(date: @date, feed: @feed.name)) %></li>
      <li><%= link_to("Next day >>>", papers_next_path(date: @date, feed: @feed.name)) %></li>
    <% end %>
  </ul>
</nav>
